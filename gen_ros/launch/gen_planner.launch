<launch>
	<arg name="op_case" default="None" />
        <arg name="case_idx" default="None" />
	<node pkg="gen_ros" type="gen_planner.py" name="planner" args="$(arg op_case) False" output="screen" />
        <node pkg="gen_ros" type="map_viewer.py" name="map_viewer" args="R_KR_PR_Naverlabs_Pangyo" output="screen" />
	<node pkg="result_checker" type="main.py" name="result_checker" args="$(arg op_case) $(arg case_idx)" output="screen" />
        <node pkg="rviz" type="rviz" name="rviz" args="-d $(find gen_ros)/rviz/planner.rviz" />

</launch>
