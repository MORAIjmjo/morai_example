<launch>
    <arg name="case_idx" default="None" />
	<node pkg="gen_ros" type="gen_planner.py" name="planner" args="9 False" output="screen" />
	<node pkg="gen_ros" type="op_9_ego_2_planner.py" name="multi_ego_planner" args="9 False" output="screen" />
	<node pkg="result_checker" type="main.py" name="result_checker" args="9 $(arg case_idx)" output="screen" />
    <node pkg="gen_ros" type="map_viewer.py" name="map_viewer" args="R_KR_PR_Naverlabs_Pangyo" output="screen" />
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find gen_ros)/rviz/planner.rviz" />

</launch>
